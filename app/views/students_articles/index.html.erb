<p id="notice"><%= notice %></p>

<h1>Students Articles</h1>

<table>
  <thead>
    <tr>
      <th>Blog</th>
      <th>User</th>
      <th>Date</th>
      <th>Blog text</th>
      <th>Blog title</th>
      <th>User img</th>
      <th>Blog link</th>
      <th>Blog img</th>
      <th>Blog upload</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @students_articles.each do |students_article| %>
      <tr>
        <td><%= students_article.date %></td>
        <td><%= students_article.blog_text %></td>
        <td><%= students_article.blog_title %></td>
        <td><%= students_article.user_img %></td>
        <td><%= students_article.blog_link %></td>
        <td><%= students_article.blog_img %></td>
        <td><%= students_article.blog_upload %></td>
        <td><%= link_to 'Show', students_article %></td>
        <td><%= link_to 'Edit', edit_students_article_path(students_article) %></td>
        <td><%= link_to 'Destroy', students_article, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
